# Nginx+FPM 
ab -k -n 2000 -c 20 http://localhost:8080/bench
ab -k -n 5000 -c 50 http://localhost:8080/bench.php
ab -k -n 10000 -c 200 http://localhost:8080/ping.txt
wrk -t8 -c50 -d30s --latency http://localhost:8080/bench.php

# Apache 
ab -k -n 2000 -c 20 http://localhost:8081/bench
ab -k -n 5000 -c 50 http://localhost:8081/bench.php
ab -k -n 10000 -c 200 http://localhost:8081/ping.txt
wrk -t8 -c50 -d30s --latency http://localhost:8081/bench.php